{
    "version": 2,
    "builds": [
        {
            "src": "vite.config.js",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "https://activity-project-mioo.onrender.com/"
          },
        {
            "src": "/api/[^.]+",
            "dest": "/api/$1"
        }
    ]
}