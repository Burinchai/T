{
    "version": 2,
    "builds": [
        {
            "src": "vite.config.js",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/[^.]+",
            "dest": "/"
        },
        {
            "src": "/api/[^.]+",
            "dest": "/api/$1"
        }
    ]
}